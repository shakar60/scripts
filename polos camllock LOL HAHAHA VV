-- Animate PolosGui entrance
local TweenInfoPolosGui = TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
local TargetPositionPolosGui = UDim2.new(0.1, 0, 0.4, 0)
