        defaultZoomDistance = LocalPlayer.CameraMaxZoomDistance
        LocalPlayer.CameraMaxZoomDistance = math.huge
    else
        LocalPlayer.CameraMaxZoomDistance = defaultZoomDistance
    end
end)

