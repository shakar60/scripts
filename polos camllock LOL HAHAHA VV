local function getCurrentPing()
    return game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()
end

local function updatePrediction()
    local ping = getCurrentPing()
    Prediction = pingValues["ping20_30"]  -- Default prediction

    if ping >= 20 and ping < 30 then
        Prediction = pingValues["ping20_30"]
    elseif ping >= 30 and ping < 40 then
        Prediction = pingValues["ping30_40"]
    elseif ping >= 40 and ping < 50 then
        Prediction = pingValues["ping40_50"]
    elseif ping >= 50 and ping < 60 then
        Prediction = pingValues["ping50_60"]
    elseif ping >= 60 and ping < 70 then
        Prediction = pingValues["ping60_70"]
    elseif ping >= 70 and ping < 80 then
        Prediction = pingValues["ping70_80"]
    elseif ping >= 80 and ping < 90 then
        Prediction = pingValues["ping80_90"]
    elseif ping >= 90 and ping < 100 then
        Prediction = pingValues["ping90_100"]
    elseif ping >= 100 and ping < 110 then
        Prediction = pingValues["ping100_110"]
    elseif ping >= 110 and ping < 120 then
        Prediction = pingValues["ping110_120"]
    elseif ping >= 120 and ping < 130 then
        Prediction = pingValues["ping120_130"]
    elseif ping >= 130 and ping < 140 then
        Prediction = pingValues["ping130_140"]
    elseif ping >= 140 and ping < 150 then
        Prediction = pingValues["ping140_150"]
    elseif ping >= 150 and ping < 160 then
        Prediction = pingValues["ping150_160"]
    elseif ping >= 160 and ping < 170 then
        Prediction = pingValues["ping160_170"]
    elseif ping >= 170 and ping < 180 then
        Prediction = pingValues["ping170_180"]
    elseif ping >= 180 and ping < 190 then
        Prediction = pingValues["ping180_190"]
    elseif ping >= 190 and ping < 200 then
        Prediction = pingValues["ping190_200"]
    elseif ping >= 200 and ping < 210 then
        Prediction = pingValues["ping200_210"]
    elseif ping >= 210 and ping < 220 then
        Prediction = pingValues["ping210_220"]
    elseif ping >= 220 and ping < 230 then
        Prediction = pingValues["ping220_230"]
    elseif ping >= 230 and ping < 240 then
        Prediction = pingValues["ping230_240"]
    elseif ping >= 240 and ping < 250 then
        Prediction = pingValues["ping240_250"]
    elseif ping >= 250 and ping < 260 then
        Prediction = pingValues["ping250_260"]
    elseif ping >= 260 and ping < 270 then
        Prediction = pingValues["ping260_270"]
    elseif ping >= 270 and ping < 280 then
        Prediction = pingValues["ping270_280"]
    elseif ping >= 280 and ping < 290 then
        Prediction = pingValues["ping280_290"]
    elseif ping >= 290 and ping < 300 then
        Prediction = pingValues["ping290_300"]
    elseif ping >= 300 and ping < 310 then
        Prediction = pingValues["ping300_310"]
    elseif ping >= 310 and ping < 320 then
        Prediction = pingValues["ping310_320"]
    elseif ping >= 320 and ping < 330 then
        Prediction = pingValues["ping320_330"]
    elseif ping >= 330 and ping < 340 then
        Prediction = pingValues["ping330_340"]
    elseif ping >= 340 and ping < 350 then
        Prediction = pingValues["ping340_350"]
    elseif ping >= 350 and ping < 360 then
        Prediction = pingValues["ping350_360"]
    elseif ping >= 360 and ping < 370 then
        Prediction = pingValues["ping360_370"]
    elseif ping >= 370 and ping < 380 then
        Prediction = pingValues["ping370_380"]
    elseif ping >= 380 and ping < 390 then
        Prediction = pingValues["ping380_390"]
    elseif ping >= 390 and ping < 400 then
        Prediction = pingValues["ping390_400"]
    elseif ping >= 400 and ping < 410 then
        Prediction = pingValues["ping400_410"]
    elseif ping >= 410 and ping < 420 then
        Prediction = pingValues["ping410_420"]
    elseif ping >= 420 and ping < 430 then
