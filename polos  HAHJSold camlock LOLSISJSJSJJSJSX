-- Set Position of the GUI
local function updateGUIPosition()
    local screenSize = workspace.CurrentCamera.ViewportSize
    Frame.Position = UDim2.new(0.5, -Frame.Size.X.Offset / 2, 0.5, -Frame.Size.Y.Offset / 2)
    MinimizedFrame.Position = UDim2.new(0.5, -MinimizedFrame.Size.X.Offset / 2, 0.5, -MinimizedFrame.Size.Y.Offset / 2)
end

updateGUIPosition()

-- Setting Up The TextBox
local function createTextBox(parent, name, displayName, positionOffset, size, placeholder, text, changeCallback, outlineColor)
    local textBox = Instance.new("TextBox")
    textBox.Parent = parent
    textBox.Name = name
    textBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    textBox.Position = positionOffset
    textBox.Size = size
    textBox.PlaceholderText = placeholder
    textBox.Text = text
    textBox.ClearTextOnFocus = false
    textBox.BorderSizePixel = 2
    textBox.BorderColor3 = outlineColor or Color3.fromRGB(0, 0, 0)
    textBox.TextScaled = true
    textBox.Font = Enum.Font.Arcade
    textBox.TextColor3 = Color3.fromRGB(0, 0, 0)
    textBox.TextStrokeTransparency = 0.8
    textBox.TextSize = 14

    local UICorner = Instance.new("UICorner")
    UICorner.Parent = textBox
    UICorner.CornerRadius = UDim.new(0, 8)

    textBox.Focused:Connect(function()
        if textBox.Text == placeholder then
            textBox.Text = ""
        end
    end)

    textBox.FocusLost:Connect(function()
        if textBox.Text == "" then
            textBox.Text = placeholder
        else
            local value = tonumber(textBox.Text)
            if value then
                changeCallback(value)
            end
        end
    end)

    return textBox
end

-- Prediction TextBox
local PredictionTextBox = createTextBox(
    Frame,
    "PredictionTextBox",
    "Enter Prediction",
    UDim2.new(0, 15, 0, 60),
