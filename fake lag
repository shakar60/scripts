local function createGUI()
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "ScreenGui"
	ScreenGui.ResetOnSpawn = false
    ScreenGui.Parent = game.CoreGui
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    local main = Instance.new("Frame")
    main.Name = "MainFrame"
    main.Parent = ScreenGui
    main.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	main.Transparency = 0.3
    main.Size = UDim2.new(0.1, 0, 0.15, 0)
    main.Position = UDim2.new(0.1, 0, 0.2, 0)
    main.Active = true
    main.Draggable = true

    local UICornerMain = Instance.new("UICorner")
    UICornerMain.CornerRadius = UDim.new(0, 25)
    UICornerMain.Parent = main

    local Title = Instance.new("TextLabel")
    Title.Name = "Title"
    Title.Parent = main
    Title.BackgroundTransparency = 1
    Title.Position = UDim2.new(0, 0, 0.1, 0)
    Title.Size = UDim2.new(1, 0, 0.2, 0)
    Title.Font = Enum.Font.Creepster
    Title.Text = "By GhostHub"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextScaled = true

    local Title2 = Instance.new("TextLabel")
    Title2.Name = "Title2"
    Title2.Parent = main
    Title2.BackgroundTransparency = 1
    Title2.Position = UDim2.new(0, 0, 0.7, 0)
    Title2.Size = UDim2.new(0.8, 0, 0.3, 0)
    Title2.Font = Enum.Font.Creepster
    Title2.Text = "Edited By\nsh4k4r6o"
    Title2.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title2.TextSize = 15

    local FakeLag = Instance.new("TextButton")
    FakeLag.Name = "FakeLag"
    FakeLag.Parent = main
    FakeLag.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    FakeLag.BackgroundTransparency = 0.5
    FakeLag.Position = UDim2.new(0.1, 0, 0.3, 0)
    FakeLag.Size = UDim2.new(0.8, 0, 0.4, 0)
    FakeLag.Font = Enum.Font.Creepster
    FakeLag.Text = "Fake Lag"
    FakeLag.TextColor3 = Color3.fromRGB(255, 255, 255)
    FakeLag.TextScaled = true
    FakeLag.TextWrapped = true

    local UICornerButton = Instance.new("UICorner")
    UICornerButton.CornerRadius = UDim.new(0, 20)
    UICornerButton.Parent = FakeLag

    local X = Instance.new("TextButton")
    X.Name = "X"
    X.Size = UDim2.new(0, 20, 0, 20)
    X.Position = UDim2.new(1, -27, 0.7, 1)
    X.TextColor3 = Color3.fromRGB(255, 255, 255)
	X.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	X.Transparency = 0.1
    X.Text = "X"
    X.Font = Enum.Font.Creepster
    X.TextSize = 14
    X.Parent = main
	
    local UICornerMain = Instance.new("UICorner")
    UICornerMain.CornerRadius = UDim.new(0, 25)
    UICornerMain.Parent = X

    local fakeLagEnabled = false
    local lagValue = 0.2


    local LagValue = Instance.new("TextLabel")
    LagValue.Name = "LagValue"
    LagValue.Parent = main
    LagValue.BackgroundTransparency = 0.3
	LagValue.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    LagValue.Position = UDim2.new(-0.6, 5, 0.1, 0)
    LagValue.Size = UDim2.new(0.5, 0, 0.5, 0)
    LagValue.Font = Enum.Font.Creepster
    LagValue.Text = "Lag Value"
    LagValue.TextColor3 = Color3.fromRGB(255, 255, 255)
    LagValue.TextSize = 12

	local UICornerMain = Instance.new("UICorner")
    UICornerMain.CornerRadius = UDim.new(0, 15)
    UICornerMain.Parent = LagValue
	
    local TextBox = Instance.new("TextBox")
    TextBox.Name = "TextBox"
    TextBox.Parent = main
    TextBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	TextBox.BackgroundTransparency = 0.9
    TextBox.Position = UDim2.new(-0.4, 0, 0.4, 0)
    TextBox.Size = UDim2.new(0.2, 0, 0.2, 0)
    TextBox.Font = Enum.Font.Creepster
    TextBox.Text = tostring(lagValue)
    TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextBox.TextScaled = true

	local UICornerMain = Instance.new("UICorner")
    UICornerMain.CornerRadius = UDim.new(0, 20)
    UICornerMain.Parent = TextBox
	
    TextBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            local inputValue = tonumber(TextBox.Text)
            if inputValue then
                lagValue = inputValue
            else
                TextBox.Text = tostring(lagValue)
            end
        end
    end)

    local function toggleFakeLag()
        fakeLagEnabled = not fakeLagEnabled
        FakeLag.Text = fakeLagEnabled and "On" or "Off"

        if fakeLagEnabled then
            getgenv().fakelag = true
            task.spawn(function()
                while getgenv().fakelag do
                    for _, V in pairs(game.Players:GetChildren()) do
                        local LocalPlayer = game:GetService("Players").LocalPlayer
                        local Character = LocalPlayer.Character
                        local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")

                        if HumanoidRootPart then
                            HumanoidRootPart.Anchored = true
                            wait(lagValue)
                            HumanoidRootPart.Anchored = false
                            wait()
                        end
                    end
                end
            end)
        else
            getgenv().fakelag = false
        end
    end

    FakeLag.MouseButton1Down:Connect(toggleFakeLag)
    X.MouseButton1Down:Connect(function()
        if fakeLagEnabled then
            toggleFakeLag()
        end
        ScreenGui:Destroy()
    end)
end

createGUI()
